{
	"hosting": {
		"public": "dist",
		"site": "igis-test-web",
		"cleanUrls": true,
		"headers": [
			{
				"source": "**/*.@(png|jpg|jpeg|gif)",
				"headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000" }]
			}
		],
		"rewrites": [
			{
				"source": "!/@(og-default.png|robots.txt|sitemap.xml)",
				"destination": "/index.html"
			}
		]
	}
}
